{
  "root": true,
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["next/core-web-vitals", "eslint:recommended", "prettier"],
  "env": {
    "es2021": true,
    "node": true
  },
  "globals": {
    "JSX": true
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
